      <resource path="~/public/scripts/uistars/jquery.ui.stars.min.css"/>

      <resource path="~/public/scripts/themeroller/Aristo.css"/>

      <resource path="~/public/styles/text.css"/>
      <resource path="~/public/styles/grid.css"/>
      <resource path="~/public/styles/main.css"/>
      <resource path="~/public/styles/theme/theme_base.css"/>

      <resource path="~/public/styles/theme/layout_fixed.css"/>
      <resource path="~/public/styles/theme/theme_blue.css"/>
      <resource path="~/public/styles/theme/bg_noise.css"/>

      <resource path="~/public/styles/buttons.css"/>
      <resource path="~/public/styles/ie.css"/>
    </resourceSet>
    <resourceSet name="adminicaJs" type="js">
