      <resource path="~/public/scripts/jquery-1.7.1.min.js" />
      <!-- Load JQuery UI -->
      <resource path="~/public/scripts/jquery-ui-1.8.17.custom.min.js" />
      <!-- Global -->
      <resource path="~/public/scripts/touchPunch/jquery.ui.touch-punch.min.js" />
      <resource path="~/public/scripts/uitotop/js/jquery.ui.totop.js" />
      <!-- Forms -->
      <resource path="~/public/scripts/uniform/jquery.uniform.min.js"/>
