      <resource path="~/scripts/i18n/grid.locale-es.js" />
      <resource path="~/scripts/jquery.jqGrid.min.js" />
      <resource path="~/scripts/jquery.blockUI.js" />
      <resource path="~/scripts/jQuery.tmpl.js" />
      <resource path="~/scripts/knockout-2.0.0.js" />
