<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" namespace="Sicemed.Web.Models" assembly="Sicemed.Web" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Turno" table="Turnos">
    <id name="Id" type="Int64">
      <generator class="hilo" />
    </id>
    <property name="FechaGeneracion" not-null="true" />
    <property name="FechaTurno" not-null="true" />
    <property name="FechaIngreso" />
    <property name="FechaAtencion" />
    <property name="FechaCancelacion" />
    <property name="Nota" />
    <property name="IpPaciente" />
    <property name="EsTelefonico" />
    <property name="Estado" />
    <property name="FechaEstado" />
    <many-to-one name="Paciente" column="PacienteId" not-null="true" foreign-key="FK_Turno_PersonaRol_Paciente" />
    <many-to-one name="Profesional" column="ProfesionalId" not-null="true" foreign-key="FK_Turno_PersonaRol_Profesional" />
    <many-to-one name="SecretariaReservadoraTurno" column="SecretariaReservadoraTurnoId" foreign-key="FK_Turno_PersonaRol_Secretaria_ReservadoraTurno" />
    <many-to-one name="SecretariaRecepcionista" column="SecretariaRecepcionistaId" foreign-key="FK_Turno_PersonaRol_Secretaria_Recepcionista" />
    <many-to-one name="CanceladoPor" column="CanceladoPorId" foreign-key="FK_Turno_Persona_CanceladoPor" />
    <many-to-one name="Especialidad" column="EspecialidadId" not-null="true" foreign-key="FK_Turno_Especialidad" />
    <many-to-one name="Consultorio" column="ConsultorioId" foreign-key="FK_Turno_Consultorio" />
  </class>
</hibernate-mapping>