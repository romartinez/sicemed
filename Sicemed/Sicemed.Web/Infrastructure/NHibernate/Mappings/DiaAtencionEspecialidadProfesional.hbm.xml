<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" namespace="Sicemed.Web.Models" assembly="Sicemed.Web" xmlns="urn:nhibernate-mapping-2.2">
  <class name="DiaAtencionEspecialidadProfesional" table="DiaAtencionEspecialidadProfesionales">
    <id name="Id" type="Int64">
      <generator class="hilo" />
    </id>
    <property name="DiaSemanaNumero" />
    <property name="DiaSemanaNombre" />
    <property name="DuracionTurno" />
    <property name="PoliticaHorariaVespertinaHasta" />
    <property name="PoliticaHorariaVespertinaDesde" />
    <property name="PoliticaHorariaMatutinaHasta" />
    <property name="PoliticaHorariaMatutinaDesde" />
    <many-to-one name="Consultorio" column="ConsultorioId" />
    <many-to-one name="EspecialidadProfesional" column="EspecialidadProfesionalId" />
    <set name="Turnos" inverse="true" cascade="all,delete-orphan">
      <key column="DiaAtencionEspecialidadProfesionalId" on-delete="cascade" />
      <one-to-many class="Turno" />
    </set>
  </class>
</hibernate-mapping>