<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" namespace="Sicemed.Web.Models" assembly="Sicemed.Web" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Calendario" table="Calendarios">
    <id name="Id" type="Int64">
      <generator class="hilo" />
    </id>
    <property name="Anio" />
    <property name="Nombre" />
    <set name="Feriados" access="field.camelcase-underscore" inverse="true" cascade="all,delete-orphan">
      <key column="CalendarioId" on-delete="cascade" />
      <one-to-many class="Feriado" />
    </set>
  </class>
</hibernate-mapping>