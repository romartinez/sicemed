<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" namespace="Sicemed.Web.Models" assembly="Sicemed.Web" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ObraSocial" table="ObraSociales">
    <id name="Id" type="Int64">
      <generator class="hilo" />
    </id>
    <property name="RazonSocial" />
    <property name="CUIT" />
    <property name="Direccion" />
    <property name="Telefono" />
    <many-to-one name="Localidad" column="LocalidadId" />
    <set name="Planes" inverse="true" cascade="all,delete-orphan">
      <key column="ObraSocialId" on-delete="cascade" />
      <one-to-many class="Plan" />
    </set>
  </class>
</hibernate-mapping>