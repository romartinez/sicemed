<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" namespace="Sicemed.Web.Models" assembly="Sicemed.Web" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Usuario" table="Usuarios">
    <id name="Id" type="Int64">
      <generator class="hilo" />
    </id>
    <component class="Sicemed.Web.Models.Components.Membership" name="Membership" access="nosetter.camelcase-underscore">
      <property name="Password" column="MembershipPassword" />
      <property name="Email" column="MembershipEmail" />
      <property name="PasswordResetToken" column="MembershipPasswordResetToken" />
      <property name="PasswordResetTokenGeneratedOn" column="MembershipPasswordResetTokenGeneratedOn" />
      <property name="IsLockedOut" column="MembershipIsLockedOut" />
      <property name="LockedOutReason" column="MembershipLockedOutReason" />
      <property name="CreateDate" access="nosetter.camelcase-underscore" column="MembershipCreateDate" />
      <property name="LastLoginDate" column="MembershipLastLoginDate" />
      <property name="LastPasswordChangedDate" column="MembershipLastPasswordChangedDate" />
      <property name="LastLockoutDate" column="MembershipLastLockoutDate" />
      <property name="FailedPasswordAttemptCount" column="MembershipFailedPasswordAttemptCount" />
      <property name="FailedPasswordAttemptWindowStart" column="MembershipFailedPasswordAttemptWindowStart" />
    </component>
    <set name="Roles" access="field.camelcase-underscore" table="UsuarioRoles">
      <key column="UsuarioId" />
      <composite-element class="Sicemed.Web.Models.Components.AsignacionRol">
        <property name="Rol" access="nosetter.camelcase-underscore" column="RolId" 
                  type="Sicemed.Web.Infrastructure.NHibernate.EnumerationType`1[[Sicemed.Web.Models.Rol, Sicemed.Web]], Sicemed.Web" />
        <property name="FechaAsignacion" access="nosetter.camelcase-underscore" />
      </composite-element>
    </set>
    <property name="FechaNacimiento" />
    <component class="Sicemed.Web.Models.Components.Documentos.Documento" name="Documento">
      <property name="Numero" column="DocumentoNumero" />
    </component>
    <component class="Sicemed.Web.Models.Components.Domicilio" name="Domicilio">
      <property name="Calle1" column="DomicilioCalle1" />
      <property name="Calle2" column="DomicilioCalle2" />
      <many-to-one name="Localidadad" column="DomicilioLocalidadadId" />
    </component>
    <property name="EstaHabilitadoTurnosWeb" />
    <component class="Sicemed.Web.Models.Components.Telefono" name="Telefono">
      <property name="Numero" column="TelefonoNumero" />
      <property name="Prefijo" column="TelefonoPrefijo" />
    </component>
    <property name="InasistenciasContinuas" />
    <property name="NumeroAfiliado" />
    <set name="Turnos" inverse="true" cascade="all,delete-orphan">
      <key column="PacienteId" on-delete="cascade" />
      <one-to-many class="Turno" />
    </set>
    <property name="Nombre" />
    <property name="SegundoNombre" />
    <property name="Apellido" />
  </class>
</hibernate-mapping>