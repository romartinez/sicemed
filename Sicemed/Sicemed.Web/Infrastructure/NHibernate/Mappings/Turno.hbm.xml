<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" namespace="Sicemed.Web.Models" assembly="Sicemed.Web" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Turno" table="Turnos">
    <id name="Id" type="Int64">
      <generator class="hilo" />
    </id>
    <property name="FechaGeneracion" />
    <property name="FechaTurno" />
    <property name="FechaIngreso" />
    <property name="FechaAtencion" />
    <property name="Nota" />
    <property name="IpPaciente" />
    <many-to-one name="DiaAtencionEspecialidadProfesional" column="DiaAtencionEspecialidadProfesionalId" />
    <many-to-one name="Paciente" column="PacienteId" />
    <many-to-one name="Secretaria" column="SecretariaId" />
  </class>
</hibernate-mapping>