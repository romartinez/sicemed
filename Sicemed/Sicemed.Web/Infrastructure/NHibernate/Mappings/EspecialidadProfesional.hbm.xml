<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" namespace="Sicemed.Web.Models" assembly="Sicemed.Web" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EspecialidadProfesional" table="EspecialidadProfesionales">
    <id name="Id" type="Int64">
      <generator class="hilo" />
    </id>
    <set name="DiaAtencionEspecialidad" inverse="true" cascade="all,delete-orphan">
      <key column="EspecialidadProfesionalId" on-delete="cascade" />
      <one-to-many class="DiaAtencionEspecialidadProfesional" />
    </set>
    <many-to-one name="Especialidad" column="EspecialidadId" />
    <many-to-one name="Profesional" column="ProfesionalId" />
  </class>
</hibernate-mapping>